{
  "name": "phantom",
  "version": "1.0.0",
  "description": "js, transport, buses, mobility",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./dist/",
    "dev": "nodemon --exec babel-node src/index.js",
    "debug": "babel-node --inspect-brk=9229 src/index.js",
    "build": "babel src --out-dir dist",
    "test": "NODE_ENV=test nyc mocha"
  },
  "mocha": {
    "spec": "src/tests/**.mjs"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "all": "true",
    "exclude": [
      "coverage"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceelogre/phantom-js.git"
  },
  "keywords": [
    "js",
    "transport",
    "buses",
    "mobility"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ceelogre/phantom-js/issues"
  },
  "homepage": "https://github.com/ceelogre/phantom-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "i18n": "^0.13.2",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  }
}
