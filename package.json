{
  "name": "phantom",
  "version": "1.0.0",
  "description": "js, transport, buses, mobility",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/",
    "dev": "nodemon node src/index.js",
    "debug": "node --inspect-brk=9229 src/index.js",
    "test": "npm run migrate:setup &&NODE_ENV=test nyc mocha",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:setup": "NODE_ENV=test npm run migrate:reset &&NODE_ENV=test npm run migrate"
  },
  "mocha": {
    "spec": "src/tests/**/**.js",
    "bail": "true",
    "exit": true
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "text",
      "lcov"
    ],
    "all": "true",
    "exclude": [
      "coverage",
      "migrations"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceelogre/phantom-js.git"
  },
  "keywords": [
    "js",
    "transport",
    "buses",
    "mobility"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ceelogre/phantom-js/issues"
  },
  "homepage": "https://github.com/ceelogre/phantom-js#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^7.11.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "i18n": "^0.13.2",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5"
  }
}
